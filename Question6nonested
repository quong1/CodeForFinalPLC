import java.util.*;

public class max_mid_min {

public static void main(String[] args) {
	int first, second, third;
  int max, min, mid;
  
	Scanner sc=new Scanner(System.in);
	System.out.print("Enter three numbers: ");
	int first=sc.nextInt();
	int second=sc.nextInt();
	int third=sc.nextInt();
	
	//from low to high
	if(third<second && second<first && third<first) { //(third, second, first)
		max=first;
		mid=second;
		min=third;
	}
	else if (third<first && first<second && third<second) { //(third, first, second) 
		max=second;
		mid=first;
		min=third;
	}
	
	else if (second<third && third<first && second<first) { //(second, third, first)
		max=first;
		mid=third;
		min=second;

}
	else if (second<first && first<third && second<third) { //(second, first, third)
		max=third;
		mid=first;
		min=second;
}
	else if (first<second && second<third && first<third) { //(first, second, third)
		max=third;
		mid=second;
		min=first;
	}
	else { //(first, third, second)
		max=second;
		mid=third;
		min=first;
	}
	System.out.println("Max is "+ max);
	System.out.println("Mid is "+ mid);
	System.out.println("Min is "+ min);

	}
	  
	}
